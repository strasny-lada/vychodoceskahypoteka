      .floor.floor--padded#floor--6
        .floor__inner
          .tabs
            ul.tabs__header
              li.tabs__header__item.tabs__header__item--active
                a.tabs__header__item__link(href="#tab--1") Kupuji nemovitost
              li.tabs__header__item
                a.tabs__header__item__link(href="#tab--2") Refinancuji hypotéku

            .tab.tab--active#tab--1
              form.form.form--tab--1
                div.tab__split.tab__split--1
                  ul.checks
                    li.check.check--pardubice
                      div
                        svg.icon.icon--pardubice
                          use(xlink:href="/img/sprite.svg#pardubice")
                      span Jsem z oblasti Pardubicka.

                    li.check.check--best
                      div
                        svg.icon.icon--best
                          use(xlink:href="/img/sprite.svg#best")
                      span Chci získat nejvýhodnější nabídku na trhu.

                    li.check.check--meeting
                      div
                        svg.icon.icon--meeting
                          use(xlink:href="/img/sprite.svg#meeting")
                      span Chci zdarma nezávaznou konzultaci.

                div.tab__split.tab__split--2
                  .form__row
                    span.label Výše hypotéky
                    #moneyValue
                    input(type="hidden" name="moneValueInput" id="moneyValueInput")

                    input(type="radio" name="hypoType" id="hypoType_1" value="kupuji" checked)
                    input(type="radio" name="hypoType" id="hypoType_2" value="refinancuji")

                  <!-- pro zobrazeni chyboveho stavu staci pridat tridu .form_row--error -->
                  .form__row.form__row--margin-top
                    input(type="text" name="name" placeholder="Karel Novák")

                  .form__row.form__row--margin-bottom
                    input(type="text" name="contact" placeholder="Telefon nebo e-mail")

                  .form__row.form__row--submit

                    <!-- pro zobrazeni staci oddelat tridu .hidden -->
                    .form__layer--error.hidden
                      p.form__layer--error__text Zapomněli jste vyplnit své jméno.

                    button.button(type="submit") Odeslat

                    span.button__info Chci získat nezávaznou&nbsp;konzultaci!

                  <!-- pro zobrazeni staci oddelat tridu .hidden -->
                  .form__layer--success.hidden
                    .form__layer--success__content
                      svg.icon.icon--check
                        use(xlink:href="/img/sprite.svg#check")
                      strong.form__layer--success__title Děkujeme
                      span.form__layer--success__text Do dalšího dne se vám ozveme.

